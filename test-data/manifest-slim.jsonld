{
  "@context": {
    "@vocab": "https://ns.flur.ee/imports#",
    "sequence": {
      "@id": "https://ns.flur.ee/imports#sequence",
      "@container": "@list"
    }
  },
  "@type": "CSVImportManifest",
  "@id": "model/bill-of-materials",
  "name": "Bill of Materials / Product Assembly Data Model",
  "description": "This data model describes an electronics hardware data specifically designed for Bill of Materials and Product Assembly",
  "model": {
    "baseIRI": "http://example.com/terms/",
    "path": "model/",
    "sequence": [
      {
        "path": "DataModel.csv",
        "@type": [
          "BasicVocabularyStep"
        ],
        "overrides": [
          {
            "column": "Class Name",
            "mapTo": "$Class.ID"
          },
          {
            "column": "Property Name",
            "mapTo": "$Property.ID"
          }
        ],
        "extraItems": [
          {
            "column": "Reasoning Logic",
            "mapTo": "http://example.com/terms/reasoningLogic",
            "onEntity": "PROPERTY"
          }
        ]
      },
      {
        "path": "MaterialClass.csv",
        "@type": [
          "SubClassVocabularyStep"
        ],
        "subClassOf": [
          "http://example.com/terms/Material"
        ],
        "replaceClassIdWith": "$Class.Name",
        "extraItems": [
          {
            "column": "Category",
            "mapTo": "http://example.com/terms/category",
            "onEntity": "CLASS"
          }
        ]
      }
    ]
  },
  "instances": {
    "baseIRI": "http://example.com/ids/",
    "path": "instances/",
    "sequence": [
      {
        "path": "Products.csv",
        "@type": [
          "BasicInstanceStep"
        ],
        "instanceType": "Product"
      },
      {
        "path": "Manufacturer.csv",
        "@type": [
          "BasicInstanceStep"
        ],
        "instanceType": "Manufacturer"
      },
      {
        "path": "WarehouseInventory.csv",
        "@type": [
          "BasicInstanceStep"
        ],
        "instanceType": "WarehouseInventory",
        "ignore": [
          "Material Name"
        ]
      },
      {
        "path": "BillOfMaterials.csv",
        "@type": [
          "BasicInstanceStep"
        ],
        "instanceType": "BillOfMaterials",
        "pivotColumns": [
          {
            "instanceType": "BillOfMaterialsItem",
            "newRelationshipProperty": "hasItems",
            "columns": [
              "quantity",
              "has Material",
              "has Material Class"
            ]
          }
        ],
        "ignore": [
          "Material Class Name",
          "Material Class Description",
          "Material Name",
          "Material Unit Price",
          "Total Material Price",
          "Min Alternative Unit Price",
          "Min Alternate Material Unit Price",
          "Minimum Alternate Price",
          "has Lowest Cost Alternative Material Number",
          "Lowest Cost Alternative Material Name",
          "Lowest Cost Alternative Material Manufacturer"
        ]
      },
      {
        "path": "Material.csv",
        "@type": [
          "BasicInstanceStep"
        ],
        "instanceType": "Material",
        "ignore": [
          "Manufacturer Name",
          "Material Class Name",
          "Lowest Cost Alternative Material Number"
        ]
      }
    ]
  }
}
