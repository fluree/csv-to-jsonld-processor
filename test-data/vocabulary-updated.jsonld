{
  "@context": {
    "f": "https://ns.flur.ee/ledger#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "rdfs:domain": {
      "@type": "@id"
    },
    "rdfs:range": {
      "@type": "@id"
    },
    "rdfs:subClassOf": {
      "@type": "@id"
    },
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "ledger": "fluree-jld/369435906932760",
  "insert": [
    {
      "@id": "dell-bom-model",
      "rdfs:label": "Manifest for Dell Bill of Materials CSV Import",
      "rdfs:comment": "This manifest describes the process for importing a set of CSV files that describe a Dell Bill of Materials Inventory",
      "@type": [
        "fluree:DataModel"
      ],
      "properties": [
        {
          "@id": "http://example.com/dell-bom/terms/bWCompetitorCost",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Competitor Retail Price and the Product Retail Price",
          "rdfs:comment": "The difference between the product suggested retail price compared to equivalent competitive product retail price.  A positive number, or better than (b), means that the competitive product retail price is larger than our suggested retail price.  A negative number, or worse than (w), means that our product retail price is higher than the equivalent competitive product retail price.  If the competitive cost is negative and the comparative competitor margins is worse (less than 1), then the product is likely poorly performing and needs to be significantly optimized.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "B/(W) Competitor Cost",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/nextShipmentReceiptDate",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The date when the next shipment of inventory is due from the Manufacturer",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/WarehouseInventory"
          ],
          "rdfs:label": "Next Shipment Receipt Date",
          "rdfs:range": [
            "xsd:date"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/hasProduct",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The specific product being described by the individual Bill of Materials record",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/BillOfMaterials"
          ],
          "rdfs:label": "has Product",
          "rdfs:range": [
            "http://example.com/dell-bom/terms/Product"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/priceRatioComparedToMax",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Max Material Unit Price and the Material Unit Price, divided by the Material Unit Price",
          "rdfs:comment": "The ratio of how much less expensive a Material is compared to the highest Material Price unit within that Material Class.  This number is always less than or equal to zero, and the lower the number, the better as this represents that a Material is distant from the maximum theoretical price for that Material Class",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Price Ratio Compared to Max",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/estCompetitorMargin",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Competitor Retail Price and the Est. Competitor Cost, divided by the Competitor Retail Price",
          "rdfs:comment": "The percentage profit income for the equivalent competitve product sold.  The idea is to be able to compare the equivalent competitive product's margins to our company.  If a competitive product has a lower retail price, but a higher comparable Gross Margin, then there are significant opportunities to improve the Product's Bill of Materials",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Est. Competitor Margin",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/productName",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The trade name of the product as it is sold across retail channels",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Product Name",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/productRetailPrice",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The suggested manufacturer's retail price for the the product.  This is the price at which the product is to be sold across retail and distribution channels to end customers.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Product Retail Price",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/bWCompetitiveRatio",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The B/(W) Competitor Cost divided by the Competitor Retail Price",
          "rdfs:comment": "The percentage difference between the difference between the Competitor Retail Price and the Product Retail Price, relative to the competitor retail price. The higher the number, the better (b) or more competitively priced our product is compared to the competition.  This could mean that we could potentially increase the suggested retail price and improve profit margins if our product Profit Margins is less than the equivalent market margin.  The lower the number, the worse (w) or more expensive our product is compared to the competition and the worse it will likely perform in the marketplace.  If this number is very low, then this product could be significantly overpriced compared to the competition which could impact sales.  We want to target this number and make it as positive as possible.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "B/(W) Competitive Ratio",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/grossMargin",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Product Retail Price and the Product Cost of Goods Sold, divided by the Product Retail Price",
          "rdfs:comment": "The percentage profit income for the product sold, based on the difference between the manufacturer's suggested retail price and its cost of goods sold.  This represents the markup percentage between the Retail Price and the Cost of Goods sold.  The higher the number, the more net income can be generated and the higher the company's financial performance",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Gross Margin",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/quantity",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The number of items from the Material Class that need to be included in the set of materials required to be assembled to generate the product.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/BillOfMaterialsItem"
          ],
          "rdfs:label": "Quantity",
          "rdfs:range": [
            "xsd:integer"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/marginImprovement",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The Potential Cost Savings Per Product divided by the Product Cost of Goods Sold",
          "rdfs:comment": "The percentage margin improvement based on comparing the potential cost save versus the current product cost.  This describes the opportunity if the lowest cost materials based on stock availability were included in the assembly.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Margin Improvement",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/hasManufacturer",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The identifier of the Manufacturer that produces the Material.  The authoritative source of the Manufacturer information is the Vendor Management System",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "has Manufacturer",
          "rdfs:range": [
            "http://example.com/dell-bom/terms/Manufacturer"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/activeSince",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The date when the contract to begin conducting business with the Manufacturer was signed",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Manufacturer"
          ],
          "rdfs:label": "Active Since",
          "rdfs:range": [
            "xsd:date"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/inventoryStatus",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "A statement as to whether the inventory supply for the Material within the Warehouse location is adequate given required batch manufacturing processes.  An Inventory Status of \"At Risk\" means that there is not sufficient inventory to assemble the product and the current batch and subsequent batches may be at risk of being assembled and shipped on time.  An Inventory Status of \"Approaching Risk\" means that inventory supply is reaching the risk threshold, but there is adequate time to manage the procurement process to replenish stock before there are impacts to production assembly lines.  An Inventory Status of \"In Stock\" means that there is adequate supply of inventory within the Warehouse location.  Finally, an Inventory Status of \"Overstock\" means that there is too much capacity for that material in inventory and speaks to either poor demand management processes, assembly line operational issues where batches are not being produced at the expected efficiency rate, or delays in entire assembly lines due to other potential materials being out of supply.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/WarehouseInventory"
          ],
          "rdfs:label": "Inventory Status",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/warehouseLocation",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The location of the Warehouse, Storage Facility or Assembly Plant where the inventory is being tracked",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/WarehouseInventory"
          ],
          "rdfs:label": "Warehouse Location",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/hasMaterial",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The identifier of the individual material selected from the vendor inventory to assemble the product.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/BillOfMaterialsItem",
            "http://example.com/dell-bom/terms/WarehouseInventory"
          ],
          "rdfs:label": "has Material",
          "rdfs:range": [
            "http://example.com/dell-bom/terms/Material"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/maxMaterialUnitPrice",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "Maximum of Material Price for all Materials within the same Material Class",
          "rdfs:comment": "The highest comparable Material Price for all Materials within the same Material Class where there is some available inventory",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Max Material Unit Price",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/competitorRetailPrice",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "Estimated average retail price for competitor products, based on industry market scan performed by the Market Intelligence team.  The idea is to be able to compare the current Product Retail Price with competitors in order to identify opportunities for price optimization",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Competitor Retail Price",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/minMaterialUnitPrice",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "Minimum of Material Price for all Materials within the same Material Class",
          "rdfs:comment": "The lowest comparable Material Price for all Materials within the same Material Class where there is some available inventory",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Min Material Unit Price",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/minimumRetailPriceAtOptimalGrossMargin",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The Minimum Potential Cost divided by one minus the higher number between the Gross Margin and the Est. Competitor Margin",
          "rdfs:comment": "The theoretical most optimal retail price if we modified the Bill of Materials to only select the lowest cost materials and set the retail price at the highest gross margin (based on comparing current versus competitos gross margin).  The retail price could be lower than the current retail price, and can be used to compare to competitor price.  This is especially valuable if the competitor's price is lower and they have a higher operating margin.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Minimum Retail Price at Optimal Gross Margin",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/manufacturerName",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The name of the Manufacturer",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Manufacturer"
          ],
          "rdfs:label": "Manufacturer Name",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/ontimeDeliveryRating",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The ontime delivery quality rating from 1 to 5 as managed by the Procurement Quality Control Team.  A score of 1 means poor product ontime delivery, meaning that shipments are consistently delayed and that the vendor does not have a good track record of inventory control and logistics management.  The Product Manager must perform a Supplier Risk Review within the calendar quarter as outlined in the Supplier Risk Management policy.  A score of 5 is the maximum possible high delivery rating, and we should continue to do more business with Manufacturers with an Ontime Delivery Rating of 4 and above.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Manufacturer"
          ],
          "rdfs:label": "Ontime Delivery Rating",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/potentialCostSavingsPerProduct",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Product Retail Price and the Minimum Retail Price at the Optimal Gross Margin",
          "rdfs:comment": "The theoretical price differential between the current Product Retail Price and the theoretical most efficient price if the lowest possible cost product was delivered at the highest margin that the market can afford.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Potential Cost Savings Per Product",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/materialDescription",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The description of the Material, as saved in the ERP and Vendor Management system",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Material Description",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/materialName",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The name of the Material.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Material Name",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/productQualityRating",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The product quality rating from 1 to 5 as managed by the Procurement Quality Control Team.  A score of 1 means poor product quality, and the Product Manager must perform a Supplier Risk Review within the calendar quarter as outlined in the Supplier Risk Management policy.  A score of 5 is the maximum possible high quality score, and we should continue to do more business with Manufacturers with a Product Quality Rating of 4 and above.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Manufacturer"
          ],
          "rdfs:label": "Product Quality Rating",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/productCostOfGoodsSold",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The sum of the Material Price for the Materials in a Product as defined by the Bill of Materials record for that Product",
          "rdfs:comment": "The total cost to manufacture the product, based on computing the sum of the cost of the underlying product components",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Product Cost of Goods Sold",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/hasLowestCostAlternativeMaterialNumber",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "Select identifier of the Material with the lowest Material Price for the Material Class ",
          "rdfs:comment": "The material identifier of the lowest possible alternative option for each Material Class.  If the material identifier is empty, this means that the material selected is the lowest costing material based on current warehouse inventory availability",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "has Lowest Cost Alternative Material Number",
          "rdfs:range": [
            "http://example.com/dell-bom/terms/Material"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/materialUnitPrice",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The unit cost of the Material.  If the Material Class is an OEM component, then this is the purchase price of the Material from the Manufacturer.  If this is a non-OEM component, then this is the internal calculated cost to internally source",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Material Unit Price",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/bWCompetitorMargins",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Gross Margin and the Est Competitor Margins",
          "rdfs:comment": "The difference between the product Gross Margin and the estimated equivalent competitive product margin.  The higher the number, the lower our cost to deliver the product compared to the competitors in the marketplace.  The lower the number, the more expensive it is to produce the product compared to the competition.  Typically, the lowest performing products that need to be optimized are the ones with a high negative competitive ratio and a negative better than (b) / worse than (w) competitor margin",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "B/(W) Competitor Margins",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/hasItems",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The identifier of the material items (material & quantity of materials) selected from the vendor inventory to assemble the product.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/BillOfMaterials"
          ],
          "rdfs:label": "has Items",
          "rdfs:range": [
            "http://example.com/dell-bom/terms/BillOfMaterialsItem"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/estCompetitorCost",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "Estimated average Cost of Goods Sold for competitor products, based on market research performed by the Market Intelligence team.  The goal is to get an understanding of competitive market components and margins, to see if there are opportunities to optimize the Product's Bill of Materials.",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Est. Competitor Cost",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/minimumPotentialCost",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The sum of the Min Alternate Material Unit Price for the Materials in a Product as defined by the Bill of Materials record for that Product",
          "rdfs:comment": "An alternative Product Cost of Goods Sold if we substituted all of the materials that make up the product assembly with the lowest possible option based on looking at parts in the inventory.  This is determined by identifying the lowest cost material option for each class of material defined for the product in the Bill of Materials",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Minimum Potential Cost",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/productDescription",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The description of the product and functionality, to be displayed in retail strorefronts and catalog.  The description is managed by the Product Management organization and recorded in the authoritative Product Lifecycle Management system",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Product"
          ],
          "rdfs:label": "Product Description",
          "rdfs:range": [
            "xsd:string"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/asOfDate",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The date as of when the inventory was tracked and last modified",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/WarehouseInventory"
          ],
          "rdfs:label": "As of Date",
          "rdfs:range": [
            "xsd:date"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/priceRatioComparedToMin",
          "@type": [
            "rdf:Property"
          ],
          "rdfs:comment": "The ratio of how much less more a Material is compared to the minimum Material Price unit within that Material Class.  This number is always greather than or equal to zero, and the higher the number, the worse as this represents that a Material is distant from the minimum theoretical price for that Material Class",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Price Ratio Compared to Min",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/totalAvailableInventory",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The sum of the Quantity Units Available for the Material across all Warehouse Locations",
          "rdfs:comment": "The sum of all current in-stock units of the Material across all current Warehouse locations, stoage facilities and assembly plants.  This number is important, as not having enough materials available for assembly could lead to lengthy product delays",
          "rdfs:domain": [
            "http://example.com/dell-bom/terms/Material"
          ],
          "rdfs:label": "Total Available Inventory",
          "rdfs:range": [
            "xsd:decimal"
          ]
        },
        {
          "@id": "http://example.com/dell-bom/terms/minimumRetailPriceBWCompetitorPrice",
          "@type": [
            "rdf:Property"
          ],
          "http://example.com/dell-bom/terms/reasoningLogic": "The difference between the Competitor Retail Price and the Minimum Retail Price at Optimal Gross Margin",
          "rdfs:comment": "The difference between the theoretical minimum retail price compared to equivalent competitive product retail price.  The goal is for this number to be higher compared to the B/(W) Competitor Cost",
          "rdfs:domain": [
